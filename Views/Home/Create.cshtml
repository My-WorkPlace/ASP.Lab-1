@model ASP.Lab_1.Data.Models.Site
@{
    ViewBag.Title = "Добавление пользователя";
}
    <form asp-action="create" asp-controller="home">
        <div class="form-group">
            <label asp-for="Name" class="control-label">Имя</label>
            <input type="text" asp-for="Name" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="UrlMainPage" class="control-label">URL path</label>
            <input type="text" asp-for="UrlMainPage" class="form-control" />
        </div>
        <div class="form-group">
            <input type="submit" value="Отправить" class="btn-outline-info"/>
        </div>
    </form>